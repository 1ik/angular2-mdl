{
  "name": "angular2-mdl",
  "version": "0.0.20",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng server",
    "postinstall": "typings install",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng version; tsc -v; ng test --watch false",
    "posttest": "remap-istanbul -i coverage/coverage-final.json -o coverage/coverage-mapped.json -t lcovonly -o coverage/lcov.info; remap-istanbul -i coverage/coverage-final.json -o coverage/coverage-mapped.json -t html -o coverage/coverage-remap",
    "pree2e": "webdriver-manager update; tsc --project e2e",
    "e2e": "protractor",
    "deploy-gh-pages": "ng github-pages:deploy --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"",
    "generate-scss-from-mdl-scss": "node ./dev-scripts/generate-scss-from-mdl-scss.js",
    "prepublish": "typings install; tsc --project src"
  },
  "main": "./dist/components/index.js",
  "typings": "./dist/components/index.d.ts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mseemann/angular2-mdl.git"
  },
  "dependencies": {
    "@angular/core": "2.0.0-rc.4",
    "@angular/common": "2.0.0-rc.4"
  },
  "devDependencies": {
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "angular-cli": "^1.0.0-beta.6",
    "broccoli-funnel": "^1.0.1",
    "broccoli-merge-trees": "^1.1.1",
    "codelyzer": "0.0.23",
    "coveralls": "^2.11.9",
    "ember-cli-inject-live-reload": "^1.4.0",
    "es6-shim": "^0.35.0",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "1.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-spec-reporter": "0.0.26",
    "material-design-lite": "1.1.3",
    "ncp": "^2.0.0",
    "node-sass": "^3.7.0",
    "protractor": "^3.3.0",
    "reflect-metadata": "0.1.3",
    "remap-istanbul": "^0.6.4",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.31",
    "ts-node": "^0.5.5",
    "tslint": "3.11.0-dev.0",
    "typescript": "1.9.0-dev.20160615-1.0",
    "typings": "^0.8.1",
    "zone.js": "^0.6.12"
  }
}
